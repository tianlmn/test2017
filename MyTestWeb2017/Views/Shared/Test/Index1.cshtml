@using MyTestWeb2017.framework
@{
    ViewBag.Title = "Home Page";
}
@model MyTestWeb2017.Models.RequestModels.BaseRequestModel

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Getting started</h2>
        <p>
            Html.ViewContext.HttpContext.Items["x"]:@Html.ViewContext.HttpContext.Items["x"]
        </p>
        <p><a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkId=301865">Learn more &raquo;</a></p>
    </div>
    <div class="col-md-4">
        @MyHtmlHelper.lll

        @Html.GroupPageRaw()
        @Html.Raw(MyHtmlHelper.lll)

        @Html.GroupPageMVCHtml()
        @Html.Raw(Html.GroupPageMVCHtml())
    </div>
    <div class="col-md-4">
        <h2>request paras</h2>
        city:@Model.CityId
        hotel:@Model.HotelId
    </div>
</div>